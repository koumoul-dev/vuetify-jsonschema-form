{
  "name": "@koumoul/vjsf-markdown",
  "version": "0.1.1",
  "description": "A plugin to add markdown edition capability to vjsf.",
  "scripts": {
    "test": "vitest",
    "build": "vue-tsc",
    "watch:build": "vue-tsc --watch"
  },
  "author": "Alban Mouton <alban.mouton@gmail.com>",
  "license": "MIT",
  "files": [
    "types",
    "src",
    "LICENSE"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "default": "./src/index.js",
        "types": "./types/index.d.ts"
      }
    },
    "./components/*": {
      "import": {
        "default": "./src/components/*",
        "types": "./types/components/*.d.ts"
      }
    }
  },
  "peerDependencies": {
    "@koumoul/vjsf": ">=3.0.0-beta",
    "vue": "^3.4.2",
    "vuetify": "^3.4.9"
  },
  "devDependencies": {
    "vitest": "^1.1.1",
    "vue": "^3.4.2",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "easymde": "^2.18.0"
  }
}
