{
  "name": "@koumoul/vjsf",
  "version": "3.0.0-beta.10",
  "description": "Generate forms for the vuetify UI library (vuejs) based on annotated JSON schemas.",
  "scripts": {
    "test": "vitest",
    "build": "vue-tsc",
    "watch:build": "vue-tsc --watch"
  },
  "author": "Alban Mouton <alban.mouton@gmail.com>",
  "license": "MIT",
  "files": [
    "types",
    "src",
    "LICENSE"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./src/index.js"
      }
    },
    "./types.js": {
      "import": {
        "types": "./types/types.d.ts",
        "default": "./src/types.js"
      }
    },
    "./components/*": {
      "import": {
        "types": "./types/components/*.d.ts",
        "default": "./src/components/*"
      }
    },
    "./composables/*": {
      "import": {
        "types": "./types/composables/*.d.ts",
        "default": "./src/composables/*"
      }
    },
    "./utils": {
      "import": {
        "types": "./types/utils/index.d.ts",
        "default": "./src/utils/index.js"
      }
    },
    "./utils/*": {
      "import": {
        "types": "./types/utils/*.d.ts",
        "default": "./src/utils/*"
      }
    },
    "./styles/*": {
      "import": {
        "default": "./src/styles/*"
      }
    },
    "./compile": {
      "import": {
        "types": "./types/compile/index.d.ts",
        "default": "./src/compile/index.js"
      }
    },
    "./compat/v2": {
      "import": {
        "types": "./types/compat/v2.d.ts",
        "default": "./src/compat/v2.js"
      }
    }
  },
  "peerDependencies": {
    "vue": "^3.4.3",
    "vuetify": "^3.4.9"
  },
  "dependencies": {
    "@json-layout/core": "0.14.0",
    "@vueuse/core": "^10.5.0",
    "debug": "^4.3.4",
    "ejs": "^3.1.9"
  },
  "devDependencies": {
    "@types/debug": "^4.1.8",
    "@types/ejs": "^3.1.2",
    "vitest": "^1.1.1",
    "vue": "^3.4.3",
    "vue-tsc": "^1.8.27"
  }
}
